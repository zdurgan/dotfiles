     User ztdurgan
     ControlMaster auto
     ControlPath ~/.ssh/_%r@%h:%p
     ControlPersist 43200

Host github
     HostName github.com
     User git
     AddKeysToAgent yes
     PreferredAuthentications publickey
     ControlMaster auto
     IdentityFile ~/.ssh/github_rsa
     ControlPersist 43200

Host gitlab.umich.edu
     User git
     Preferredauthentications publickey
